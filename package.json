{
    "name": "m7-shell",
    "version": "0.28.2",
    "private": true,
    "dependencies": {
        "@algont/m7-keyboard": "^0.1.0",
        "@algont/m7-react-marker": "^0.1.2",
        "@algont/m7-shell-emitter": "^0.11.1",
        "@algont/m7-ui": "^0.12.9",
        "@algont/m7-url-transformer": "^0.1.0",
        "@algont/m7-utils": "^0.4.2",
        "@date-io/moment": "2.11.0",
        "@material-ui/core": "^4.12.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.60",
        "axios": "^0.22.0",
        "classnames": "^2.3.1",
        "history": "^5.0.1",
        "js-base64": "^3.7.2",
        "lodash": "^4.17.21",
        "mobx": "^6.3.3",
        "mobx-react": "^7.2.0",
        "mobx-undecorate": "^1.1.0",
        "moment": "^2.29.1",
        "object-hash": "^2.2.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-draggable": "^4.4.4",
        "react-localization": "^1.0.17",
        "react-resizable": "^3.0.4",
        "react-router": "^5.2.1",
        "react-router-dom": "^5.3.0",
        "react-scripts": "^4.0.3",
        "react-svg": "^14.0.14",
        "socket.io-client": "^2.4.0",
        "uuid": "^8.3.2"
    },
    "scripts": {
        "start": "react-scripts start",
        "stage": "npm version patch --git-tag-version=false",
        "build": "react-scripts build",
        "build:env": "cd ./scripts && npx ts-node ./build.ts",
        "build:night": "cross-env REACT_APP_NIGHT=true react-scripts build",
        "deploy": "ts-node ./scripts/deploy.ts",
        "prettier": "prettier --write \"./**/*.{ts,tsx,css}\"",
        "prettier-watch": "onchange 'src/**/*.{ts,tsx,css}' -- prettier --write {{changed}}",
        "lint": "eslint ./src/**/*.{ts,tsx}",
        "fix": "eslint ./src/**/*.{ts,tsx} --fix",
        "nexus": "npm publish ./build",
        "doc": "typedoc"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{ts, tsx, css}": [
            "npm run prettier",
            "npm run lint",
            "git add"
        ]
    },
    "devDependencies": {
        "@types/jest": "27.0.2",
        "@types/lodash": "^4.14.175",
        "@types/node": "16.10.2",
        "@types/object-hash": "^2.2.1",
        "@types/react": "^17.0.27",
        "@types/react-dom": "17.0.9",
        "@types/react-resizable": "^1.7.3",
        "@types/react-router-dom": "^5.3.0",
        "@types/socket.io-client": "^1.4.36",
        "@types/storybook-readme": "^5.0.5",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^4.33.0",
        "@typescript-eslint/eslint-plugin-tslint": "^4.33.0",
        "@typescript-eslint/parser": "^4.33.0",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "eslint": "^7.32.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "lint-staged": "^11.2.0",
        "onchange": "^7.1.0",
        "prettier": "^2.4.1",
        "ts-node": "^10.2.1",
        "tslint-config-prettier": "^1.18.0",
        "typedoc": "^0.22.5",
        "typescript": "^4.4.3"
    },
    "homepage": "./"
}
