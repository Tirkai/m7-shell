{
    "name": "m7-shell",
    "version": "0.21.0",
    "private": true,
    "dependencies": {
        "@algont/m7-shell-emitter": "^0.7.0",
        "@algont/m7-ui": "^0.9.1",
        "@algont/m7-utils": "^0.3.0",
        "@date-io/moment": "1.3.13",
        "@material-ui/core": "^4.11.0",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.52",
        "@types/jest": "24.0.18",
        "@types/node": "12.7.8",
        "@types/react": "16.9.4",
        "@types/react-dom": "16.9.1",
        "@types/socket.io-client": "^1.4.33",
        "@typescript-eslint/eslint-plugin": "^2.31.0",
        "@typescript-eslint/parser": "^2.31.0",
        "axios": "^0.19.2",
        "classnames": "^2.2.6",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "history": "^4.10.1",
        "js-base64": "^3.4.4",
        "lodash": "^4.17.15",
        "mobx": "^6.0.1",
        "mobx-react": "^7.0.0",
        "mobx-undecorate": "^1.0.2",
        "moment": "^2.26.0",
        "object-hash": "^2.0.3",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-draggable": "^4.3.1",
        "react-localization": "^1.0.15",
        "react-resizable": "^1.10.1",
        "react-router": "^5.1.2",
        "react-router-dom": "^5.1.2",
        "react-scripts": "^3.4.1",
        "react-svg": "^11.0.36",
        "socket.io-client": "^2.3.0",
        "tslint-to-eslint-config": "^0.3.1",
        "typescript": "^3.8.3",
        "uuid": "^7.0.3"
    },
    "scripts": {
        "start": "react-scripts start",
        "stage": "npm version patch --git-tag-version=false",
        "build": "react-scripts build",
        "build:env": "cd ./scripts && npx ts-node ./build.ts",
        "deploy": "ts-node ./scripts/deploy.ts",
        "prettier": "prettier --write \"./**/*.{ts,tsx,css}\"",
        "prettier-watch": "onchange 'src/**/*.{ts,tsx,css}' -- prettier --write {{changed}}",
        "lint": "eslint ./src/**/*.{ts,tsx}",
        "fix": "eslint ./src/**/*.{ts,tsx} --fix",
        "storybook:dev": "start-storybook",
        "storybook:build": "build-storybook",
        "nexus": "npm publish ./build"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "husky": {
        "hooks": {
            "pre-commit": "npm run lint"
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*.{ts, tsx, css}": [
            "npm run prettier",
            "npm run lint",
            "git add"
        ]
    },
    "devDependencies": {
        "@dump247/storybook-state": "^1.6.1",
        "@storybook/addon-actions": "^5.3.18",
        "@storybook/addon-viewport": "^5.3.18",
        "@storybook/react": "^5.3.18",
        "@types/classnames": "^2.2.10",
        "@types/lodash": "^4.14.150",
        "@types/object-hash": "^1.3.1",
        "@types/react-resizable": "^1.7.2",
        "@types/react-router-dom": "^5.1.5",
        "@types/storybook-readme": "^5.0.3",
        "@types/uuid": "^7.0.3",
        "@typescript-eslint/eslint-plugin-tslint": "^2.31.0",
        "chalk": "^4.1.0",
        "cross-env": "^7.0.2",
        "husky": "^3.0.9",
        "lint-staged": "^9.4.2",
        "onchange": "^6.1.1",
        "prettier": "^1.19.1",
        "ssh2-sftp-client": "^5.3.1",
        "storybook-addon-figma": "^0.1.0",
        "storybook-readme": "^5.0.8",
        "ts-node": "^9.0.0",
        "tslint": "^5.20.0",
        "tslint-config-prettier": "^1.18.0"
    },
    "homepage": "./"
}
